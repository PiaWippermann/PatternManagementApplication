{
    "compilerOptions": {
      "jsx": "react-jsx",
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "types": ["vite/client", "react", "react-dom"]
    }
